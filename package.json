{
  "name": "healproBackend",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev --env staging ",
    "deploy": "wrangler deploy --minify",
    "typegen": "wrangler types --env staging",
    "db:push": "drizzle-kit push --config=drizzle-dev.config.ts",
    "db:push:staging": "drizzle-kit push --config=drizzle-staging.config.ts",
    "db:push:prod": "drizzle-kit push --config=drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle-dev.config.ts",
    "db:migrate:staging": "drizzle-kit migrate --config=drizzle-staging.config.ts",
    "db:migrate:prod": "drizzle-kit migrate --config=drizzle.config.ts",
    "db:generate": "drizzle-kit generate --config=drizzle-dev.config.ts",
    "db:generate:staging": "drizzle-kit generate --config=drizzle-staging.config.ts",
    "db:generate:prod": "drizzle-kit generate --config=drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=drizzle-dev.config.ts",
    "db:studio:staging": "drizzle-kit studio --config=drizzle-staging.config.ts",
    "db:drop:staging": "drizzle-kit drop --config=drizzle-staging.config.ts",
    "db:pull:staging": "drizzle-kit pull --config=drizzle-staging.config.ts",
    "db:studio:prod": "drizzle-kit studio --config=drizzle.config.ts"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "better-auth": "^1.3.34",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "env": "^0.0.2",
    "hono": "^4.10.4",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "drizzle-kit": "0.31.6",
    "wrangler": "^4.4.0"
  }
}
